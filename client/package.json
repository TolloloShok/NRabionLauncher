{
  "name": "NRabionLauncher",
  "version": "1.0.8",
  "description": "Minecraft Launcher for [N]-Rabion",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --asar=true --overwrite --platform=win32 --arch=ia32 --prune=true --out=release-builds --icon=icon.ico",
    "pack": "build --dir --ia32",
    "dist": "build --ia32"
  },
  "build": {
    "appId": "nrabion.launcher",
    "mac": {
      "category": "nrabion.launcher.minecraft"
    },
    "icon": "icon.ico"
  },
  "repository": "https://github.com/kapehh-karen/NRabionLauncher",
  "keywords": ["minecraft"],
  "author": "kapehh",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "*",
    "electron-packager": "*",
    "electron-builder": "*"
  },
  "dependencies": {
    "extract-zip": "*",
    "jquery": "*",
    "nconf": "*",
    "fs-extra": "*",
    "request": "*"
  }
}
